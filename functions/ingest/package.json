{
  "name": "@roakit/ingest-function",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:proto && tsc",
    "build:proto": "mkdir -p src/generated && protoc --ts_opt=esModuleInterop=true --ts_opt=json_names --ts_out=src/generated proto/*.proto",
    "watch": "tsc --watch",
    "local": "npm run build && firebase use eternal-impulse-412418 && firebase emulators:start --inspect-functions --only functions:ingest",
    "local:no-write": "cross-env NO_WRITE=true npm run local",
    "deploy": "npm run build && firebase use eternal-impulse-412418 && firebase deploy --only functions:ingest",
    "deploy:prod": "npm run build && firebase use prod && firebase deploy --only functions:ingest && npm run smoke",
    "lint": "eslint --ext .ts .",
    "tsc": "tsc",
    "test": "jest",
    "smoke": "scripts/smoke-test.sh",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^5.6.0",
    "@koa/router": "^12.0.1",
    "async-retry": "^1.3.3",
    "config": "^3.3.12",
    "firebase": "^10.12.3",
    "firebase-admin": "^12.2.0",
    "firebase-functions": "^5.0.1",
    "google-protobuf": "^3.21.2",
    "koa": "^2.15.3",
    "node-cache": "^5.1.2",
    "pino": "^9.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.8.1",
    "@types/async-retry": "^1.4.8",
    "@types/config": "^3.3.4",
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.12",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.4",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "google-artifactregistry-auth": "^3.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "protoc-gen-ts": "^0.8.7",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": "20"
  }
}